<!-- Folders section -->
<h4><%= file.name %></h4>

<table class="table table-borderless">
    <tbody>
        <tr>
            <td>Id</td>
            <td><%= file.id %></td>
        </tr>
        <tr>
            <td>Size</td>
            <td><%= file.size %>B</td>
        </tr>
        <tr>
            <td>Upload Time</td>
            <td><%= file.createdAt %></td>
        </tr>
        
    </tbody>
</table>

<a href="<%= file.url  %>" download="<%= file.name %>">
    <button class="btn btn-dark">Download</button>
</a>